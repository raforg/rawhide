test_rawhide "$rh -L '%.5n\n'           $d/f" "00001\n"                                    ""                                         0 "-L '%.5n\\\\n'"
test_rawhide "$rh -L '%25.5n\n'         $d/f" "                    00001\n"                ""                                         0 "-L '%25.5n\\\\n'"
